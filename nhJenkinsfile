pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                sudo podman build --tag 52.79.48.121:30002/nh-project/nhimage:test .
            }
        }
    }
}

    stages {
        stage('harbor push') {
            steps {
		sudo podman push 52.79.48.121:30002/nh-project/nhimage
            }
        }
    }
}
